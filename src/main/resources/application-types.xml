<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="input" format="csv">
      <types:shape format="weave" example="examples/employee.csv"><![CDATA[%dw 2.0
type input = Array<{|
  id: String, 
  name: String, 
  department: String
|}> {"MetadataFormat": "csv"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
        <types:property name="separator" value=","/>
      </types:properties>
    </types:type>
  </types:catalog>
  <types:enrichment select="#94fffbf0-8d2d-4123-94bc-f64a5a79587d">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="input"/>
        </types:message>
      </types:input-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>